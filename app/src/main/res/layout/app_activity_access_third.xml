<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <bike.gymproject.viewlibray.ItemView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/itemview_google_fit"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp50"
        android:layout_marginTop="@dimen/dp1"
        android:background="@drawable/common_item_bg_selector"
        android:onClick="onClick"
        android:visibility="visible"
        app:contentSize="@dimen/sp14"
        app:contentTextColor="@color/common_tips_color"
        app:itemText="@string/google_fit"
        app:itemviewTitleSize="@dimen/sp15"
        app:remindIcon="@drawable/icon_google_fit"
        app:showArrow="true"
        app:showBottomLine="false"
        app:showCheckModel="false"
        app:showRemind="true" />


</RelativeLayout>
